          </ProgressBarStyle>
          <ProgressValue>#TV.Record.percent2</ProgressValue>
        </Control>
        <Control x:type="ProgressBar">
          <Name>ProgressBar</Name>
          <Id>9</Id>
          <Width>560</Width>
          <Height>37</Height>
          <PosX>360</PosX>
          <PosY>652</PosY>
          <VisibleCondition />
          <Animations />
          <MediaPortalFocusControls />
          <ProgressBarStyle>
            <BorderThickness>0,0,0,0</BorderThickness>
            <CornerRadius>0,0,0,0</CornerRadius>
            <BackgroundBrush />
            <BorderBrush />
            <ShadeBrush />
            <GlossBrush />
            <BarMargin>2,2,2,2</BarMargin>
            <BarBorderThickness>2,2,2,2</BarBorderThickness>
            <BarCornerRadius>11,11,11,11</BarCornerRadius>
            <BarBorderBrush />
            <BarBackgroundBrush x:type="GradientBrush">
              <Angle>Vertical</Angle>
              <GradientStops>
                <GradientStop Color="#70ED2E2E" Offset="0.194" />
                <GradientStop Color="#FF4E0000" Offset="1" />
              </GradientStops>
            </BarBackgroundBrush>
          </ProgressBarStyle>
          <ProgressValue>#TV.Record.percent3</ProgressValue>
